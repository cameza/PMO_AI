{
  "rewrites": [
    {
      "source": "/api/py/:path*",
      "destination": "/api/index.py"
    }
  ],
  "functions": {
    "api/index.py": {
      "runtime": "@vercel/python@4.5.0",
      "maxDuration": 60,
      "excludeFiles": "{slack-bot/**,data/**,docs/**,.agent/**,test_*.py,**/*.test.py,**/test_*.py,**/__pycache__/**,backend/venv/**,backend/scripts/**}"
    }
  }
}
